<class name="wxMenuBar">
    <class name="wxWindow"/>
    <parents>
        <class name="wxFrame">
    </parents>
    <children>
        <class name="wxMenu"/>
    </children>
    <properties>
        <name>m_menuBar</name>
        <styles name="Style">
            <style name="wxMB_DOCKABLE">
                <description>The menu bar can be detached (wxGTK only).</description>
            </style>
        </styles>
    </properties>
</class>