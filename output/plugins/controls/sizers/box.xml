<class name="wxBoxSizer">
    <class name="wxSizer"/>
    <properties>
        <name>boxSizer</name>
        <styles name="orient" label="Orientation">
            <style name="wxHORIZONTAL"/>
            <style name="wxVERTICAL"/>
