<library name="Sizers">
    <class name="button">
        <children>
            <class name="wxButton"/>
            1
        </children>
    </class>

    <class name="wxStdDialogButtonSizer">
        <class name="wxSizer">
        <children>
            <class name="button"/>
        </children>
        <properties>
            <name>sdbSizer</name>
            <text name="label">
                <description>Label to be used for the static box around the sizer (required).</description>
            </text>
        </properties>
    </class>
</library>