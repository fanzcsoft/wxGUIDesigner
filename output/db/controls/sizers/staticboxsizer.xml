<class name="wxStaticBoxSizer" type="sizer">
    <inherits>
        <class>wxBoxSizer</class>
    </inherits>
    <children>
        <class name="sizeritem" max="-1"/>
        <class name="spacer"    max="-1"/>
    </children>
    <name label="Name">
        <value>sbSizer</value>
    </name>
    <text name="label">
        <description>Label to be used for the static box around the sizer (required).</description>
    </text>
</class>