<class name="wxPanel" type="container">
    <description>A panel is a window on which controls are placed.</description>
    <inherits>
        <class>wxWindow</class>
    </inherits>
    <children>
        <class name="control" max="-1"/>
        <class type="sizer"   max="1"/>
    </children>
    <name label="Name">
        <value>m_panel</value>
    </name>
    <event name="wxNavigationKeyEvent">
        <type name="wxEVT_NAVIGATION_KEY">
            <macro>EVT_NAVIGATION_KEY</macro>
            <description>Process a navigation key event.</description>
        </type>
    </event>
</class>