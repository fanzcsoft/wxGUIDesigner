<plugin name="Top Level">
    <class name="wxWizardPage">
        <inherits>
            <class>wxPanel</class>
        </inherits>
        <parents>
            <class>wxWizard</class>
        </parents>
        <name>m_wizPage</name>
        <bitmap name="Bitmap">
            <description>Page-specific bitmap (default: none).</description>
        </bitmap>
    </class>

    <class name="wxWizardPageSimple">
        <inherits>
            <class>wxWizardPage</class>
        </inherits>
    </class>
</plugin>