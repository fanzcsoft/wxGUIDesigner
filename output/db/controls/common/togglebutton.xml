<class name="wxToggleButton">
    <inherits>
        <class>wxAnyButton</class>
    </inherits>
    <name>m_toggleBtn</name>
    <bool name="checked">
        <description>Should the button be checked/pressed initially (default: 0)?</description>
    </bool>
    <event name="wxCommandEvent">
        <type name="EVT_TOGGLEBUTTON">
            <description>Handles a wxEVT_COMMAND_TOGGLEBUTTON_CLICKED event.</description>
        </type>
    </event>
</class>