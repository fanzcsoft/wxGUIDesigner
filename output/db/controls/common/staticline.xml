<class name="wxStaticLine">
    <description>A static line is just a line which may be used in a dialog to separate the groups of controls.</description>
    <inherits>
        <class>wxWindow</class>
    </inherits>
    <name label="Name">
        <value>m_statLine</value>
    </name>
    <style label="Style">
        <flag name="wxLI_HORIZONTAL">
            <description>Creates a horizontal line.</description>
        </flag>
        <flag name="wxLI_VERTICAL">
            <description>Creates a vertical line.</description>
        </flag>
    </style>
</class>