<class name="wxStaticBitmap">
    <description>A static bitmap control displays a bitmap.</description>
    <inherits>
        <class>wxWindow</class>
    </inherits>
    <name label="Name">
        <value>m_statBmp</value>
    </name>
    <bitmap label="Bitmap">
        <description>Bitmap to display (required).</description>
    </bitmap>
</class>